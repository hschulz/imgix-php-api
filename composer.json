{
    "name": "hschulz/imgix-php-api",
    "description": "imgix php api",
    "type": "library",
    "keywords": ["imgix", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hauke Schulz",
            "email": "hauke27@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hschulz\\Imgix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hschulz\\Imgix\\Test\\": "Tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "require": {
        "php": "^7.4.0 | ^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --bootstrap=./vendor/autoload.php --whitelist=./src/ --testdox ./Tests/"
    }
}
